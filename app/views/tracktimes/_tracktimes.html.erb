<table class="table">
  <thead>
    <tr>
      <th scope="col">Project</th>
      <th scope="col">description</th>
      <th scope="col">start time</th>
      <th scope="col">end time</th>
      <th scope="col">duration</th>
    </tr>
  </thead>
  <tbody>
    <% @tracktimes.each do |tracktime| %>
    <tr>
      <td><%#= tracktime.project.name %>Project nameが入る予定</td>
      <td><%= tracktime.description %></td>
      <td><%= tracktime.start_time %></td>
      <td><%= tracktime.end_time %></td>
      <td><% if tracktime.end_time %>
          <%= tracktime.end_time - tracktime.start_time %>
          <% else %>
          <%= nil %>
          </td>
          <% end %>
    </tr>
    <% end %>
  </tbody>
</table>